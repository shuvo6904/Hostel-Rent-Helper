    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E8E7E3"
    tools:context=".adminFragment.UserListFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.recyclerview.widget.RecyclerView
